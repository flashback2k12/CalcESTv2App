<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView02"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clickable="true" >

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="727dp"
    android:background="@drawable/background_activities" >

    <TextView
        android:id="@+id/etLine1"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium"
        tools:ignore="TextViewEdits" />

    <TextView
        android:id="@+id/etLine3"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine2"
        android:layout_below="@+id/etLine2"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine4"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine3"
        android:layout_below="@+id/etLine3"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium"
        tools:ignore="TextViewEdits" />

    <TextView
        android:id="@+id/etLine5"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine4"
        android:layout_below="@+id/etLine4"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine1"
        android:layout_below="@+id/etLine1"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine6"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine5"
        android:layout_below="@+id/etLine5"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine7"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine6"
        android:layout_below="@+id/etLine6"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine8"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine7"
        android:layout_below="@+id/etLine7"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine9"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine8"
        android:layout_below="@+id/etLine8"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine9"
        android:layout_below="@+id/etLine9"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/btnCreateFile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:onClick="btnCreateFile"
        android:text="@string/button_datei" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine1"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:text="@string/auswertung"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/btnAuswertung"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:text="@string/button_auswertung" 
        android:onClick="btnAuswertung"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etLine10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="200dp"
        android:text="@string/createfileact"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine11"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine10"
        android:layout_below="@+id/etLine10"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine12"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine11"
        android:layout_below="@+id/etLine11"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine13"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine12"
        android:layout_below="@+id/etLine12"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/etLine14"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etLine13"
        android:layout_below="@+id/etLine13"
        android:layout_marginTop="10dp"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left"
        android:text="@string/tvLines"
        android:textAppearance="?android:attr/textAppearanceMedium" >

        <requestFocus />
    </TextView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnCreateFile"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/sd_pfad"
        android:textAppearance="?android:attr/textAppearanceSmall" />

</RelativeLayout>

</ScrollView>